@media screen and (max-width: 767px)
	
	aside
		position initial
		display initial
		height 100%
		width auto
		
	.dropdown-toggle
		left 20px
	.form-control
		width 95%
		margin-left 10px
	.navbar-left
		width auto
		background #BDBDBD
		
	.navbar-collapse 
		padding 0
		overflow hidden
	.navbar-heder
		color black
		
	.cont
		margin 0
		
		
@media screen and (max-width: 450px)
	.datos
		text-align center
		font-size 15px
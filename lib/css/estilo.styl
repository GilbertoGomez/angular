@font-face {
	font-family: 'iconos';
	src:url('fonts/icomoon.eot?-etyqoi');
	src:url('fonts/icomoon.eot?#iefix-etyqoi') format('embedded-opentype'),
		url('fonts/icomoon.woff?-etyqoi') format('woff'),
		url('fonts/icomoon.ttf?-etyqoi') format('truetype'),
		url('fonts/icomoon.svg?-etyqoi#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}


aside
	position fixed
	height 100%
	background #1C1C1C
	left 0
	//border-right 50px solid black
body
	padding-top 0
	margin 0 auto
	padding 0
	height 100%
		
footer
	background #1C1C1C
	color white
	
span
	margin-left 20px
	font-size 20px
	
.contenedor
	width 100%
	
.cont
	position inherit
	margin 0px 0px 0px 250px
	padding 0px 30px
	border-left 1px solid #E7E7E7
	
.barra
	background transparent
	
.navbar-left
	background transparent
	width 250px
	height 100%
	
.pie
	position relative
	background black
	color red
	
.datos
	display flex
	position relative
	justify-content space-between
	margin-top 5px
	font-size 25px

.holaa
	position absolute
	background #222
	display block
	top 50px
	height 2100px	 
	
.navbar-fixed-top
	position fixed
	
.navbar-heder
	color white
	
.navbar-default
	background transparent
	border-color transparent
	
.navbar
	margin-bottom 0
	
.navbar-inverse
	
	 border 0
	
.nav-tabs
	position block
	border-color transparent
	
	li
		width 100%
		
		font-size 15px
		font-weight bold
		
		
#grafica
	position relative
	padding 0 10px 0 10px
	left -15px

#chart-area
	position relative
	padding 0 10px 0 10px
